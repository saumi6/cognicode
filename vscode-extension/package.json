{
    "name": "cogni-client",
    "displayName": "CogniClient",
    "description": "VS Code Client for Cognicode Relational Testing",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:python",
        "workspaceContains:test_repo"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "cogni.checkConnection",
                "title": "Cogni: Check Server Connection"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npx tsc -p ./",
        "watch": "npx tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/node": "18.x",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "axios": "^1.6.0"
    }
}